{
  "name": "vue-todo-list",
  "version": "1.0.0",
  "description": "一个基于Vue 3 + TypeScript的现代化待办事项管理应用",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:check": "vue-tsc --noEmit && vite build",
    "build:analyze": "vue-tsc --noEmit && vite build --config vite.config.analyze.ts",
    "build:report": "node scripts/build-report.js",
    "build:prod": "npm run build:report",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .vue,.js,.ts",
    "lint:fix": "eslint src --ext .vue,.js,.ts --fix",
    "format": "prettier --write src/**/*.{vue,ts,js,css,md}"
  },
  "dependencies": {
    "pinia": "^2.1.7",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.2",
    "@vue/vue3-jest": "^29.2.6",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.1.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "terser": "^5.43.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2",
    "vite": "^5.0.0",
    "vite-bundle-analyzer": "^0.7.0",
    "vue-tsc": "^2.0.0"
  },
  "keywords": [
    "vue3",
    "typescript",
    "todo-list",
    "pinia",
    "vite",
    "frontend"
  ],
  "author": "Vue Todo List Project",
  "license": "MIT"
}
